! Xft settings
Xft.antialias:          true
Xft.rgba:               rgb
Xft.hinting:            true
Xft.autohint:           true
Xft.hintstyle:          hintslight
Xft.lcdfilter:          lcddefault
Xft*dpi:                96

URxvt.font:             xft:Iosevka:style=Medium:size=11
URxvt.scrollBar:        true
URxvt.scrollBar_right:  true
URxvt.scrollstyle:      plain
URxvt.thickness:        3
URxvt*cursorUnderline:  true

URxvt.perl-ext-common: default,matcher,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

!URxvt*depth: 32
!URxvt*transparent: true
!URxvt*shading: 5

URxvt.colorUL: #5f8191

*background: #1b1b1b
*foreground: #B7BBB7

! black
*color0: #2C2F33
*color8: #4B5056

! red
*color1: #B04C50
*color9: #B04C50

! green
*color2: #919652
*color10: #94985B

! yellow
*color3: #E2995C
*color11: #E2995C

! blue
*color4: #66899D
*color12: #66899D

! magenta
*color5: #8D6494
*color13: #8D6494

! cyan
*color6: #527C77
*color14: #527C77

! white
*color7: #606360
*color15: #DDE3DC
